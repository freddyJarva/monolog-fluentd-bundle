{
    "name": "vt/monolog-fluentd-bundle",
    "description": "Symfony2 bundle defining a fluentd handler for monolog",
    "homepage": "https://github.com/freddyJarva/monolog-fluentd-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "FJ",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || >=7.4",
        "fluent/logger": "v1.0.*",
        "symfony/monolog-bundle": "^2.12 || ^3.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "phpunit/phpunit": "9.5.*",
        "symfony/var-dumper": "^2.0 || ^3.0",
        "symfony/yaml": "3.4"
    },
    "scripts": {
        "check-cs": [
            "php-cs-fixer -vvv fix --dry-run"
        ],
        "check-sec": [
            "security-checker -vv security:check"
        ],
        "check": [
            "@check-cs",
            "@check-sec"
        ],
        "fix-cs": [
            "php-cs-fixer -vvv fix"
        ]
    },
    "autoload": {
        "psr-4": {
            "VT\\MonologFluentdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VT\\MonologFluentdBundle\\Tests\\": "tests/"
        }
    }
}
